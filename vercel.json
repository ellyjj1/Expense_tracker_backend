{
    "builds": [
      {
        "src": "expense_tracker_backend/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "expense_tracker_backend/wsgi.py"
      }
    ]
  }